# Copy this file to .env and fill in your values.
# cp .env.example .env

# Your root domain â€” Matrix user IDs will look like @user:SERVER_NAME
SERVER_NAME=example.com

# The subdomain where tuwunel is reachable (must have a DNS A record)
MATRIX_SUBDOMAIN=matrix.example.com

# The subdomain for Element Call / Matrix RTC (must have a DNS A record)
RTC_SUBDOMAIN=matrix-rtc.example.com

# Secret token required for registration. Pick something long and random:
#   openssl rand -base64 32
REGISTRATION_TOKEN=change-me

# TURN server shared secret (must match coturn.conf static-auth-secret):
#   openssl rand -base64 32
TURN_SECRET=change-me

# LiveKit credentials (must match livekit.yaml keys section)
# Key: ~20 random alphanumeric chars:  openssl rand -hex 10
# Secret: ~64 random chars:            openssl rand -base64 48
LIVEKIT_KEY=change-me
LIVEKIT_SECRET=change-me

# Maubot admin UI password (set before first run)
MAUBOT_ADMIN_PASSWORD=change-me
